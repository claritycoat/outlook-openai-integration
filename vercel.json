{
  "version": 2,
  "builds": [
    {
      "src": "api/email_processor.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/test.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/simple.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/email_processor",
      "dest": "/api/email_processor.py"
    },
    {
      "src": "/api/test",
      "dest": "/api/test.py"
    },
    {
      "src": "/api/simple",
      "dest": "/api/simple.py"
    }
  ]
} 